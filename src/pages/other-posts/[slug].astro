---
import { getCollection, getEntry } from 'astro:content';
import Base from "../../layouts/Base.astro";
import RelatedPosts from "../../components/RelatedPosts.astro"
import ExtandedSideBar from "../../components/ExtandedSideBar.astro"
import TableOfContent from  "../../components/TableOfContent.astro"
import SideTableOfContent from  "../../components/SideTableOfContent.astro"
import dayjs from "dayjs";
import utc from "dayjs/plugin/utc";
dayjs.extend(utc);

export async function getStaticPaths() {
  const blogEntries = await getCollection('blog');
  
  return blogEntries.map(( blog) => {
    return {
      params: { slug: blog.slug },
      props: { blog },
    };
  });
 
}

const { blog } = Astro.props;
const { Content, headings , remarkPluginFrontmatter } = await blog.render();
const lastModified = dayjs(remarkPluginFrontmatter.lastModified)
  .utc()
  .format("DD MMMM YYYY");
